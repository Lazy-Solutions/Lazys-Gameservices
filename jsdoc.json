{
    "source": {
        "include": ["Cores/GameserverCore/core.js", "Cores/MatchmakingCore/core.js"],
        "includePattern": ".js$",
        "exclude": ["node_modules", "bin", "certificates"]
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "bin/docs"
    }
}
